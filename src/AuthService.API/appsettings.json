{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "AuthService",
    "Audience": "AuthServiceClient",
    "PrivateKeyPath": "Keys/private.key",
    "PublicKeyPath": "Keys/public.key",
    "ExpiryMinutes": 60
  }
}
